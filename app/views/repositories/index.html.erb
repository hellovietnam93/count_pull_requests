<div class="row">
  <div class="col-md-12">
    <table class="table table-border">
      <thead>
        <tr>
          <th>#</th>
          <th><%= t ".name" %></th>
        </tr>
      </thead>
      <tbody>
        <% @repositories.each_with_index do |repository, index| %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= link_to repository.name, repository %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
